<Window x:Class="Mobile5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:Mobile5.ViewModels"
        xmlns:v="clr-namespace:Mobile5.Views"
        
        xmlns:local="clr-namespace:Mobile5"
        mc:Ignorable="d"
        Title="MainWindow" Height="779" Width="1529">
    <TabControl>
        
        <TabItem Header="Город">
            <v:CityView/>
        </TabItem>
        
        <TabItem Header="Компания">
            <v:CompanyView/>
        </TabItem>


        <TabItem Header="Работник">
            <v:EmployeeView/>
        </TabItem>

        <TabItem Header="Позиция">
            <v:PositionView/>
        </TabItem>

        <TabItem Header="Транспортировка">
            <v:TransportView/>
        </TabItem>

        <TabItem Header="Тип заказа">
            <v:TypeCargo/>
        </TabItem>
        
    </TabControl>
</Window>
